<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.shardcraft.ssl.SslConfigController">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <HBox>
            <Label text="Path to Truststore">
                <padding>
                    <Insets right="20.0"/>
                </padding>
            </Label>
            <TextField fx:id="truststorePathField"/>
        </HBox>
        <HBox>
            <Label text="Truststore Password">
                <padding>
                    <Insets right="20.0"/>
                </padding>
            </Label>
            <TextField fx:id="truststorePasswordField"/>
        </HBox>
    </VBox>
</AnchorPane>
